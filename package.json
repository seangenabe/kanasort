{
  "name": "kanasort",
  "version": "1.0.0",
  "description": "Sort Japanese text.",
  "main": "lib/kanasort.js",
  "bin": {
    "kanasort": "bin/kanasort"
  },
  "scripts": {
    "test": "node --harmony node_modules/mocha/bin/_mocha lib/test",
    "start": "node --harmony .",
    "compile": "babel -d lib --source-maps --optional runtime --stage 0 src",
    "watch": "babel -w -d lib --source-maps --optional runtime --stage 0 src",
    "prepublish": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seangenabe/kanasort.git"
  },
  "keywords": [
